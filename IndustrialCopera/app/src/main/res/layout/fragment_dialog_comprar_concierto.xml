<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout22"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".User.Dialog.DialogComprarConcierto">

    <TextView
        android:id="@+id/TV_DC_Titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/B_DC_Mas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/TV_DC_Cantidad"
        app:layout_constraintTop_toTopOf="@+id/B_DC_Menos" />

    <TextView
        android:id="@+id/TV_DC_Cantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/B_DC_Menos"
        app:layout_constraintEnd_toStartOf="@+id/B_DC_Mas"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/B_DC_Menos"
        app:layout_constraintTop_toTopOf="@+id/B_DC_Menos" />

    <Button
        android:id="@+id/B_DC_Menos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:text="Button"
        app:layout_constraintEnd_toStartOf="@+id/TV_DC_Cantidad"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_Titulo" />

    <TextView
        android:id="@+id/TV_DC_Cupon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/S_DC_Cupon"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_Cantidad" />

    <Spinner
        android:id="@+id/S_DC_Cupon"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/B_DC_Mas"
        app:layout_constraintStart_toStartOf="@+id/B_DC_Menos"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_Cupon" />

    <TextView
        android:id="@+id/TV_DC_Titulo_PrecioO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/S_DC_Cupon"
        app:layout_constraintTop_toBottomOf="@+id/S_DC_Cupon" />

    <TextView
        android:id="@+id/TV_DC_PrecioO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="97dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/S_DC_Cupon"
        app:layout_constraintStart_toEndOf="@+id/TV_DC_Titulo_PrecioO"
        app:layout_constraintTop_toTopOf="@+id/TV_DC_Titulo_PrecioO" />

    <TextView
        android:id="@+id/TV_DC_Titulo_PrecioC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/TV_DC_Titulo_PrecioO"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_Titulo_PrecioO" />

    <TextView
        android:id="@+id/TV_DC_PrecioC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/S_DC_Cupon"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_PrecioO" />

    <Button
        android:id="@+id/B_DC_Comprar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Comprar entradas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TV_DC_PrecioC" />

</androidx.constraintlayout.widget.ConstraintLayout>